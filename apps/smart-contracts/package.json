{
  "name": "smart-contracts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "account": "hardhat account",
    "accounts": "hardhat accounts",
    "balance": "hardhat balance",
    "dev": "ts-node scripts/start.ts && hardhat node --network hardhat --export-all ../frontend/generated/contracts/hardhat_contracts.json",
    "compile": "hardhat compile --force",
    "deploy": "hardhat deploy --reset --export-all ../frontend/generated/contracts/hardhat_contracts.json",
    "generate": "hardhat generate",
    "postdeploy": "hardhat run scripts/publish.ts",
    "send": "hardhat send",
    "test": "hardhat test --network hardhat",
    "watch": "ts-node scripts/watch.ts"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "2.1.0",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.7.1",
    "@tenderly/hardhat-tenderly": "1.1.4",
    "chai": "4.3.6",
    "chalk": "4.1.0",
    "dotenv": "16.0.1",
    "ethers": "5.6.9",
    "hardhat": "2.10.1",
    "hardhat-abi-exporter": "2.10.0",
    "hardhat-deploy": "0.11.12",
    "hardhat-gas-reporter": "1.0.8",
    "node-watch": "0.7.3",
    "shared-constants": "*",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "10.1.0",
    "@types/node": "18.6.1",
    "solium": "1.2.5",
    "solium-config-dyor": "*",
    "tsconfig": "*",
    "typechain": "8.1.0",
    "typescript": "4.7.4"
  }
}
